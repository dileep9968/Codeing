#include<iostream>
using namespace std;

int main(){
    
    int sum=0;
    int n;
    cin>>n;            // initialize the size of array
    int arr[n];        // Declare the array of size n
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];    //get input from keyboard
    }
    for(int i=0;i<n;i++)
    {
        sum += arr[i];  //process the sum
    }
    cout<<sum;          //print sum of elements
    
}
